- title "Keys"
= render 'form'
- for key in @keys
  .item
    .ui-widget.ui-helper-clearfix{:style => "float:right"}
      = link_to "", edit_key_path(key), :class => 'ui-icon	ui-icon-pencil edit tip', :title => "Edit This Item"
      = link_to "", key, :confirm => 'Are you sure?', :method => :delete, :class => 'ui-icon ui-icon-close destroy tip', :title => "Destroy This Item"
    %h3= h key.word
