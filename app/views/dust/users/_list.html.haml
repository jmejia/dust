= will_paginate @users
- for user in @users
  .item
    .ui-widget.ui-helper-clearfix{:style => "float:right"}
      = link_to "", edit_user_path(user), :class => 'ui-icon	ui-icon-pencil edit tip', :title => "Edit This Item"
      = link_to "", user, :confirm => 'Are you sure?', :method => :delete, :class => 'ui-icon ui-icon-close destroy tip', :title => "Destroy This Item"
    %h3= link_to user.username, user
= will_paginate @users
